{
  "name": "firstch",
  "version": "0.1.0",
  "description": "First CH5 Program",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url .",
    "build:archive": "ch5-cli archive -p FirstCh -d dist -o tsw -c ./assets/contract/Contract1.cse2j",
    "build:deploy": "ch5-cli deploy -H 192.168.0.105 -t touchscreen ./tsw/FirstCh.ch5z ",
    "build:all": "rm tsw -rf && npm run build && npm run build:archive && npm run build:deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DGI-Technologies/FirstCH.git"
  },
  "author": "Alvaro Yurrita",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DGI-Technologies/FirstCH/issues"
  },
  "homepage": "https://github.com/DGI-Technologies/FirstCH#readme",
  "dependencies": {
    "@crestron/ch5-crcomlib":"^1.1.0",
    "@crestron/ch5-theme": "^1.0.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  }
}
